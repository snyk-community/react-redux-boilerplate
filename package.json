{
  "name": "ligastavok",
  "private": true,
  "version": "0.0.0",
  "main": "server/index.js",
  "scripts": {
    "start": "DEBUG=app:*info,app:*warn,app:*error,app:*debug NODE_ENV=production CONFIG=production node `pwd`/server/index.js",
    "build": "better-npm-run build",
    "webpack": "webpack",
    "server": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p",
      "env": {
        "CONFIG": "production",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node server/index.js",
      "env": {
        "CONFIG": "development",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack-dev-server.js",
      "env": {
        "CONFIG": "development",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-core": "6.10.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "better-npm-run": "0.0.8",
    "body-parser": "1.15.0",
    "compression": "1.6.1",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "express": "4.13.4",
    "pretty-error": "2.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "3.0.0",
    "redux": "3.3.1",
    "redux-async-connect": "1.0.0-rc2",
    "redux-thunk": "2.0.1",
    "scroll-behavior": "0.3.3",
    "serialize-javascript": "1.2.0",
    "webpack-isomorphic-tools": "2.2.44"
  },
  "devDependencies": {
    "autoprefixer": "6.3.4",
    "babel-core": "6.6.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-react-hmre": "1.1.1",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "deep-merge": "1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "image-webpack-loader": "1.6.3",
    "img-loader": "1.2.2",
    "json-loader": "0.5.4",
    "piping": "0.3.0",
    "postcss-color-alpha": "1.0.3",
    "postcss-flexbugs-fixes": "2.0.0",
    "postcss-loader": "0.8.2",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "1.2.0",
    "postcss-vertical-rhythm": "1.1.5",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.2",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.9",
    "redux-logger": "2.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-md5-hash": "0.0.5"
  }
